{
    "magic_Levels_of_mastery_and_abilities": {
        "description": "Levels of mastery and A comprehensive list of all magical abilities, descriptions, and associations with specific characters, clans, or courts.",
        "levels_of_mastery": {
            "class": {
                "None": {
                    "description": "No knowledge or experience with the skill or magic.",
                    "traits": [
                        "Unable to use the ability.",
                        "Requires guidance or external help.",
                        "Unaware of potential power or how to tap into it."
                    ]
                },
                "Novice": {
                    "description": "The character has just begun learning the ability. They understand its basic principles but lack control or finesse.",
                    "traits": [
                        "Occasional success with small effects.",
                        "Prone to mistakes and unintended consequences.",
                        "Requires a mentor or guide to develop further."
                    ]
                },
                "Awakened": {
                    "description": "The character becomes aware of their deeper potential. They have unlocked the ability and can use it more intentionally.",
                    "traits": [
                        "Abilities become more reliable.",
                        "Can execute intermediate techniques with concentration.",
                        "First signs of a personal connection with the ability.",
                        "May experience surges or fluctuations as they adjust to their power."
                    ]
                },
                "Divine": {
                    "description": "Mastery that reflects the domain of gods, where the ability is powerful and refined but still bound by certain rules.",
                    "traits": [
                        "Can wield abilities with ease and precision.",
                        "Powers gain significance on a spiritual or divine level.",
                        "Connection to ancient or godly forces may appear."
                    ]
                },
                "Supreme": {
                    "description": "The character has reached an apex of control and power, wielding the ability with unmatched expertise.",
                    "traits": [
                        "Rarely experiences failure with the ability.",
                        "Perfect synergy between skill and magic.",
                        "Capable of using powers creatively.",
                        "Can perform feats that inspire awe or reshape reality on a localized scale."
                    ]
                },
                "Legendary": {
                    "description": "This level represents an almost mythic mastery. The character becomes a figure of legend, transcending mortal comprehension.",
                    "traits": [
                        "Abilities reshape regions, destinies, or the flow of time.",
                        "Magic use becomes instinctive, almost effortless.",
                        "Feats become immortalized in stories and prophecies.",
                        "Seen as the embodiment of a particular force or ideal."
                    ]
                }
            }
        },            
